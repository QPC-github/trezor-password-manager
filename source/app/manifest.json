{
  "name": "TREZOR Guard",
  "description": "Really good and secure password manager working with your TREZOR device. All files synced and encrypted on Dropbox. Is there anything better? Enjoy!",
  "version": "0.0.1",
  "author": "SatoshiLabs",
  "short_name": "TREZOR Guard",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+G"
      },
      "description": "Open TREZOR Guard app window."
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "TREZOR Password",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/dropbox.min.js",
      "js/trezor.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "html/chrome_oauth_receiver.html"
  ],
  "externally_connectable": {
    "ids": [
      "jcjjhjgimijdkoamemaghajlhegmoclj"
    ]
  },
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "chrome://favicon/",
    "storage",
    "background",
    "tabs",
    "activeTab",
    "nativeMessaging",
    "https://*.dropbox.com/*"
  ]
}
