{
  "name": "Keezor",
  "description": "Keezor",
  "version": "1.0.6",
  "author": "SatoshiLabs",
  "short_name": "Keezor",

  "manifest_version": 2,

  "browser_action": {
    "default_icon": {  
      "19": "icon19.png", 
      "38": "icon38.png"
    },
    "default_title": "Keezor open",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]

}
